<?php

$list = array_keys($tree->inheritance, $parent);
sort($list, SORT_STRING|SORT_FLAG_CASE);

if(empty($list))
{
	return;
}

echo '<ul>';
foreach($list as $class)
{
	$this->view->partial('classes-list-item', array(
		'class'          => $class,
		'implementation' => $implementation,
		'inheritance'    => $inheritance,
		'children'       => true
	));
}
echo '</ul>';
